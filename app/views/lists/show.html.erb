<h1>List Details</h1>

<h3><%= @list.name %></h3>

<% @list.bookmarks.each do |bookmark| %>
<ul>
<li>Title: <%= bookmark.movie.title %></li>
<li>Comment: <%= bookmark.comment %></li>
<li> <%= bookmark.id %></li>
</ul>

  <%= link_to "Delete", bookmark_path(bookmark), data: {
    turbo_method: :delete} %>
<% end %>
<%= link_to 'Back to lists', lists_path %>
